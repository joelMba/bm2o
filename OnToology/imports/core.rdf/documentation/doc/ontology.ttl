@prefix : <https://w3id.org/rec/core/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <https://w3id.org/rec/core/> .

<https://w3id.org/rec/core/> rdf:type owl:Ontology ;
                              owl:versionIRI <https://w3id.org/rec/core/3.3/> ;
                              owl:imports <https://w3id.org/rec/metadata/3.3/> ,
                                          <https://w3id.org/rec/units/3.3/> ;
                              <http://creativecommons.org/ns#license> <https://opensource.org/licenses/MIT> ;
                              <http://purl.org/dc/elements/1.1/creator> "Erik Wallin" ,
                                                                        "Karl Hammar" ;
                              <http://purl.org/dc/elements/1.1/description> """The REC core module collects the top-level classes and properties that span over or are reused within multiple REC modules.

Note that this module reuses certain classes, properties, and named individuals from other vocabularies, e.g., GeoSPARQL; the copyright conditions on those reused entities are stated in their respective rdfs:comments annotations."""@en ;
                              <http://purl.org/dc/elements/1.1/publisher> "RealEstateCore Consortium" ;
                              <http://purl.org/dc/elements/1.1/title> "RealEstateCore Core Module" ;
                              <http://purl.org/dc/terms/modified> "2020-06-23" ;
                              <http://purl.org/vocab/vann/preferredNamespacePrefix> "core" ;
                              <http://purl.org/vocab/vann/preferredNamespaceUri> "https://w3id.org/rec/core/3.3/" ;
                              <http://www.ontologydesignpatterns.org/schemas/cpannotationschema.owl#coversRequirements> "How many rooms are there in Building 1?"@en ,
                                                                                                                        "What does electricity meter 3 measure? In what units?"@en ,
                                                                                                                        "Where in Building 1 (i.e., in which rooms) are there devices mounted?"@en ,
                                                                                                                        "Where is Building 1 located?"@en ,
                                                                                                                        "Where is the real estate Ulvsnaes 1:7 located?"@en ,
                                                                                                                        "Which buildings and land make up the real estate Ulvsnaes 1:7?"@en ,
                                                                                                                        "Which parts of Building 1 (e.g., rooms, wings, etc) are covered by electricity meter 3?"@en ;
                              rdfs:seeAlso <https://realestatecore.io> ;
                              owl:versionInfo "3.3" .

#################################################################
#    Datatypes
#################################################################

###  http://www.opengis.net/ont/geosparql#gmlLiteral
<http://www.opengis.net/ont/geosparql#gmlLiteral> rdf:type rdfs:Datatype ;
                                                  rdfs:comment """A GML serialization of a geometry object.

GeoSPARQL is Copyright (c) 2012 Open Geospatial Consortium, Inc. All Rights Reserved. http://www.opengeospatial.org/ogc/document."""@en ;
                                                  rdfs:label "GML Literal"@en .


###  http://www.opengis.net/ont/geosparql#wktLiteral
<http://www.opengis.net/ont/geosparql#wktLiteral> rdf:type rdfs:Datatype ;
                                                  rdfs:comment """A Well-known Text serialization of a geometry object.

GeoSPARQL is Copyright (c) 2012 Open Geospatial Consortium, Inc. All Rights Reserved. http://www.opengeospatial.org/ogc/document."""@en ;
                                                  rdfs:label "Well-known Text Literal"@en .


###  http://www.w3.org/2001/XMLSchema#duration
xsd:duration rdf:type rdfs:Datatype .


#################################################################
#    Object Properties
#################################################################

###  http://www.opengis.net/ont/geosparql#defaultGeometry
<http://www.opengis.net/ont/geosparql#defaultGeometry> rdf:type owl:ObjectProperty ;
                                                       rdfs:subPropertyOf <http://www.opengis.net/ont/geosparql#hasGeometry> ;
                                                       rdfs:range <http://www.opengis.net/ont/geosparql#Geometry> ;
                                                       rdfs:comment """The default geometry to be used in spatial calculations. It is Usually the most detailed geometry.

GeoSPARQL is Copyright (c) 2012 Open Geospatial Consortium, Inc. All Rights Reserved. http://www.opengeospatial.org/ogc/document."""@en ;
                                                       rdfs:isDefinedBy <http://www.opengis.net/ont/geosparql#> ;
                                                       rdfs:label "default geometry"@en .


###  http://www.opengis.net/ont/geosparql#hasGeometry
<http://www.opengis.net/ont/geosparql#hasGeometry> rdf:type owl:ObjectProperty ;
                                                   rdfs:range <http://www.opengis.net/ont/geosparql#Geometry> ;
                                                   rdfs:comment """A spatial representation for a given feature.

GeoSPARQL is Copyright (c) 2012 Open Geospatial Consortium, Inc. All Rights Reserved. http://www.opengeospatial.org/ogc/document."""@en ;
                                                   rdfs:isDefinedBy <http://www.opengis.net/ont/geosparql#> ;
                                                   rdfs:label "has geometry"@en .


###  https://w3id.org/rec/core/architectedBy
:architectedBy rdf:type owl:ObjectProperty ;
               rdfs:domain :Space ;
               rdfs:range :Agent ;
               rdfs:label "architected by"@en .


###  https://w3id.org/rec/core/categorizationProperties
:categorizationProperties rdf:type owl:ObjectProperty ;
                          rdfs:range :PropertySet ;
                          rdfs:label "categorization properties"@en .


###  https://w3id.org/rec/core/componentOf
:componentOf rdf:type owl:ObjectProperty ;
             owl:inverseOf :hasBuildingComponent ;
             rdfs:domain :BuildingComponent ;
             rdfs:range :Space ;
             rdfs:comment "Links a Building Component to a Space, that the Building Component is either contained in, adjacent to or intersecting with."@en ;
             rdfs:label "component of"@en .


###  https://w3id.org/rec/core/componentOfBuilding
:componentOfBuilding rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf :componentOf ;
                     rdfs:domain :BuildingComponent ;
                     rdfs:range :Building ;
                     rdfs:comment "Indicates which Building a certain Building Component is part of."@en ;
                     rdfs:label "component of building"@en .


###  https://w3id.org/rec/core/constructedBy
:constructedBy rdf:type owl:ObjectProperty ;
               rdfs:domain :Space ;
               rdfs:range :Agent ;
               rdfs:label "constructed by"@en .


###  https://w3id.org/rec/core/containsMountedDevice
:containsMountedDevice rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf :hasPart ;
                       owl:inverseOf :isMountedIn ;
                       rdfs:comment "Parthood traversal property linking to mounted Devices."@en ;
                       rdfs:label "contains mounted device"@en ;
                       owl:deprecated "true"^^xsd:boolean .


###  https://w3id.org/rec/core/controlledBy
:controlledBy rdf:type owl:ObjectProperty ;
              rdfs:domain :Capability ;
              rdfs:range :Capability ;
              rdfs:label "controlled by"@en .


###  https://w3id.org/rec/core/dataSchema
:dataSchema rdf:type owl:ObjectProperty ;
            rdfs:range :DataSchema ;
            rdfs:label "data schema"@en .


###  https://w3id.org/rec/core/deviceMeasurementUnit
:deviceMeasurementUnit rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf :hasMeasurementUnit ;
                       rdfs:range :MeasurementUnit ;
                       rdfs:comment """The measurement unit used by some Device.

Deprecated in favour of qudt:unit"""@en ;
                       rdfs:label "device measurement unit"@en ;
                       owl:deprecated "true"^^xsd:boolean .


###  https://w3id.org/rec/core/deviceQuantityKind
:deviceQuantityKind rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf :hasQuantityKind ;
                    rdfs:range :QuantityKind ;
                    rdfs:comment """The Quantity Kind that a device measures or affects.

Deprecated in favour of qudt:hasQuantityKind"""@en ;
                    rdfs:label "device quantity kind"@en ;
                    owl:deprecated "true"^^xsd:boolean .


###  https://w3id.org/rec/core/documentTopic
:documentTopic rdf:type owl:ObjectProperty ;
               owl:inverseOf :hasDocument ;
               rdfs:domain :Document ;
               rdfs:label "document topic"@en .


###  https://w3id.org/rec/core/eventMeasurementUnit
:eventMeasurementUnit rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf :hasMeasurementUnit ;
                      rdfs:domain :Event ;
                      rdfs:range :MeasurementUnit ;
                      rdfs:comment """A unit used to measure some aspect of some Event.

Deprecated in favour of qudt:unit"""@en ;
                      rdfs:label "event measurement unit"@en ;
                      owl:deprecated "true"^^xsd:boolean .


###  https://w3id.org/rec/core/eventQuantityKind
:eventQuantityKind rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf :hasQuantityKind ;
                   rdfs:domain :Event ;
                   rdfs:range :QuantityKind ;
                   rdfs:comment """The Quantity Kind that an event measures or affects.

Deprecated in favour of qudt:hasQuantityKind"""@en ;
                   rdfs:label "event quantity kind"@en ;
                   owl:deprecated "true"^^xsd:boolean .


###  https://w3id.org/rec/core/generatedObservation
:generatedObservation rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf :producedEvent ;
                      owl:inverseOf :observationGeneratedBy ;
                      rdfs:range :Observation ;
                      rdfs:label "generated observation"@en .


###  https://w3id.org/rec/core/hasBuildingComponent
:hasBuildingComponent rdf:type owl:ObjectProperty ;
                      owl:inverseOf :isPartOfBuilding ;
                      rdfs:domain :Space ;
                      rdfs:range :BuildingComponent ;
                      rdfs:comment "Links a Space to a BuildingComponent that is either contained in, adjacent to or intersecting with the Space."@en ;
                      rdfs:label "has building component"@en .


###  https://w3id.org/rec/core/hasCapability
:hasCapability rdf:type owl:ObjectProperty ;
               owl:inverseOf :isCapabilityOf ;
               rdfs:range :Capability ;
               rdfs:label "has capability"@en .


###  https://w3id.org/rec/core/hasDocument
:hasDocument rdf:type owl:ObjectProperty ;
             rdfs:range :Document ;
             rdfs:label "has document"@en .


###  https://w3id.org/rec/core/hasGeoReferenceOrigo
:hasGeoReferenceOrigo rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf <http://www.opengis.net/ont/geosparql#hasGeometry> ;
                      rdfs:domain :Building ;
                      rdfs:range :GeoReferenceOrigo ;
                      rdfs:comment "Associates a building with a point in some coordinate reference system which can be used to resolve local (building-specific) coordinates against this outer coordinate reference system."@en ;
                      rdfs:label "has geo-reference origo"@en .


###  https://w3id.org/rec/core/hasMeasurementUnit
:hasMeasurementUnit rdf:type owl:ObjectProperty ;
                    rdfs:range :MeasurementUnit ;
                    rdfs:comment """The unit that something (sensor observations, device capabilities, physical measurements, etc) is measured by. This is given as an object property with a corresponding class (rather than a simple datatype property) since one unit may have many serialized representations and may be used for many different types of measurements.

Deprecated in favour of qudt:unit"""@en ;
                    rdfs:label "has measurement unit"@en ;
                    owl:deprecated "true"^^xsd:boolean .


###  https://w3id.org/rec/core/hasMember
:hasMember rdf:type owl:ObjectProperty ;
           owl:inverseOf :isMemberOf ;
           rdfs:domain :Collection ;
           rdfs:comment "has member"@en ;
           owl:deprecated "true"^^xsd:boolean .


###  https://w3id.org/rec/core/hasPart
:hasPart rdf:type owl:ObjectProperty ;
         owl:inverseOf :isPartOf ;
         rdfs:comment "Points to sub-entities that share the same base type (i.e., Spaces only have Spaces as parts, Assets only have Assets as parts, etc.)."@en ;
         rdfs:label "has part"@en .


###  https://w3id.org/rec/core/hasQuantityKind
:hasQuantityKind rdf:type owl:ObjectProperty ;
                 rdfs:range :QuantityKind ;
                 rdfs:comment """Links entities that are measurable to the Quantity Kind that they measure, e.g., Temperature, Pressure, etc.

Deprecated in favour of qudt:hasQuantityKind"""@en ;
                 rdfs:label "quantity kind"@en ;
                 owl:deprecated "true"^^xsd:boolean .


###  https://w3id.org/rec/core/hasRealEstateComponent
:hasRealEstateComponent rdf:type owl:ObjectProperty ;
                        rdfs:subPropertyOf :hasPart ;
                        owl:inverseOf :isPartOfRealEstate ;
                        rdfs:domain :RealEstate ;
                        rdfs:range :RealEstateComponent ;
                        rdfs:comment "Parthood traversal property linking a piece of RealEstate to its constituent components (i.e., Buildings and Land)."@en ;
                        rdfs:label "has real estate component"@en ;
                        owl:deprecated "true"^^xsd:boolean .


###  https://w3id.org/rec/core/hasSubBuildingComponent
:hasSubBuildingComponent rdf:type owl:ObjectProperty ;
                         owl:inverseOf :hasSuperBuildingComponent ;
                         rdfs:domain :BuildingComponent ;
                         rdfs:range :BuildingComponent ;
                         rdfs:comment "Parthood traversal property linking BuildingComponents to their constituent sub-components."@en ;
                         rdfs:label "has sub-BuildingComponent"@en ;
                         owl:deprecated "true"^^xsd:boolean .


###  https://w3id.org/rec/core/hasSubDevice
:hasSubDevice rdf:type owl:ObjectProperty ;
              owl:inverseOf :hasSuperDevice ;
              rdfs:comment "Parthood traversal property linking a Device to any sub-Devices that it contains."@en ;
              rdfs:label "has sub-device"@en ;
              owl:deprecated "true"^^xsd:boolean .


###  https://w3id.org/rec/core/hasSuperBuildingComponent
:hasSuperBuildingComponent rdf:type owl:ObjectProperty ;
                           rdfs:domain :BuildingComponent ;
                           rdfs:range :BuildingComponent ;
                           rdfs:comment "Property to model nesting of building components, e.g., rooms within apartments or walls within stairwells, etc."@en ;
                           rdfs:label "has super-BuildingComponent"@en ;
                           owl:deprecated "true"^^xsd:boolean .


###  https://w3id.org/rec/core/hasSuperDevice
:hasSuperDevice rdf:type owl:ObjectProperty ;
                rdfs:comment "A property that allows for nesting of devices within devices."@en ;
                rdfs:label "has super-Device"@en ;
                owl:deprecated "true"^^xsd:boolean .


###  https://w3id.org/rec/core/hasType
:hasType rdf:type owl:ObjectProperty ;
         rdfs:range :Type ;
         rdfs:comment "A generic typing relation, holding between individuals and their types (which are also expressed as individuals). This construct allows implementors to add their own types without modifying the ontology and its classes. See the Explicit Typing design pattern in the MODL design library for further discussion."@en ;
         rdfs:label "has type"@en ;
         owl:deprecated "true"^^xsd:boolean .


###  https://w3id.org/rec/core/hostedBy
:hostedBy rdf:type owl:ObjectProperty ;
          owl:inverseOf :hosts ;
          rdfs:domain :LogicalDevice ;
          rdfs:range :Asset ;
          rdfs:comment "Indicates the physical hardware asset that a given logical device is hosted and executed on."@en ;
          rdfs:label "hosted by"@en .


###  https://w3id.org/rec/core/hosts
:hosts rdf:type owl:ObjectProperty ;
       rdfs:domain :Asset ;
       rdfs:range :LogicalDevice ;
       rdfs:comment "Indicates that a given Asset hosts a logical device; e.g., a Raspberry Pi hosts a Home Assistant installation, or an IoT-connect smart camera unit hosts an IoT Edge runtime."@en ;
       rdfs:label "hosts"@en .


###  https://w3id.org/rec/core/includedIn
:includedIn rdf:type owl:ObjectProperty ;
            owl:inverseOf :includes ;
            rdfs:range :Collection ;
            rdfs:comment "Indicates that an entity is included in some Collection, e.g., a Building is included in a RealEstate, or a Room is included in an Apartment."@en ;
            rdfs:label "included in"@en .


###  https://w3id.org/rec/core/includes
:includes rdf:type owl:ObjectProperty ;
          rdfs:domain :Collection ;
          rdfs:comment "Member entities of a collection; e.g., a Campus includes some Space, an Apartment includes some Room, etc."@en ;
          rdfs:label "includes"@en .


###  https://w3id.org/rec/core/isCapabilityOf
:isCapabilityOf rdf:type owl:ObjectProperty ;
                rdfs:domain :Capability ;
                rdfs:comment "The entity (Asset, Space, LogicalDevice, etc.) that has this Capability."@en ;
                rdfs:label "is capabilty of"@en .


###  https://w3id.org/rec/core/isLocationOf
:isLocationOf rdf:type owl:ObjectProperty ;
              owl:inverseOf :locatedIn ;
              rdfs:domain :Space ;
              rdfs:label "is location of"@en .


###  https://w3id.org/rec/core/isMemberOf
:isMemberOf rdf:type owl:ObjectProperty ;
            rdfs:range :Collection ;
            rdfs:label "is member of"@en ;
            owl:deprecated "true"^^xsd:boolean .


###  https://w3id.org/rec/core/isMountedIn
:isMountedIn rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf :isPartOf ;
             rdfs:comment "Property describing where a Device is mounted."@en ;
             rdfs:label "is mounted in"@en ;
             owl:deprecated "true"^^xsd:boolean .


###  https://w3id.org/rec/core/isMountedInBuildingComponent
:isMountedInBuildingComponent rdf:type owl:ObjectProperty ;
                              rdfs:subPropertyOf :isMountedIn ;
                              rdfs:range :BuildingComponent ;
                              rdfs:comment "Property describing in which Building Component (most typically a Room) that a Device is mounted."@en ;
                              rdfs:label "is mounted in Building Component"@en ;
                              owl:deprecated "true"^^xsd:boolean .


###  https://w3id.org/rec/core/isPartOf
:isPartOf rdf:type owl:ObjectProperty ;
          rdfs:comment "Indicates a super-entity of the same base type (i.e., Spaces only have Spaces as parents, Organizations only have Organizations, etc)."@en ;
          rdfs:label "is part of"@en .


###  https://w3id.org/rec/core/isPartOfBuilding
:isPartOfBuilding rdf:type owl:ObjectProperty ;
                  rdfs:domain :BuildingComponent ;
                  rdfs:range :Building ;
                  rdfs:comment """Indicates which Building a certain Building Component is part of.

Deprecated in favour of core:componentOfBuilding"""@en ;
                  rdfs:label "is part of building"@en ;
                  owl:deprecated "true"^^xsd:boolean .


###  https://w3id.org/rec/core/isPartOfRealEstate
:isPartOfRealEstate rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf :isPartOf ;
                    rdfs:domain :RealEstateComponent ;
                    rdfs:range :RealEstate ;
                    rdfs:comment "Indicates that a Building or a piece of Land belongs to / is part of a given Real Estate."@en ;
                    owl:deprecated "true"^^xsd:boolean .


###  https://w3id.org/rec/core/locatedIn
:locatedIn rdf:type owl:ObjectProperty ;
           rdfs:range :Space ;
           rdfs:label "located in"@en .


###  https://w3id.org/rec/core/observationGeneratedBy
:observationGeneratedBy rdf:type owl:ObjectProperty ;
                        rdfs:subPropertyOf :producedBy ;
                        rdfs:domain :Observation ;
                        rdfs:label "observation generated by"@en .


###  https://w3id.org/rec/core/observedBy
:observedBy rdf:type owl:ObjectProperty ;
            owl:inverseOf :observes ;
            rdfs:comment "Generic property indicating which sensors that are configured or designed to observe some feature, system, phenomenon, etc."@en ;
            rdfs:label "observed by"@en .


###  https://w3id.org/rec/core/observes
:observes rdf:type owl:ObjectProperty ;
          rdfs:comment "Indicates the (optional) entity-of-interest of some sensor, i.e., what it is configured to observe or look for. This allows systems to perform graph propagation of sensor values, e.g., letting a temperature sensor that observes a given room set the temperature for that room."@en ;
          rdfs:label "observes"@en .


###  https://w3id.org/rec/core/operatedBy
:operatedBy rdf:type owl:ObjectProperty ;
            rdfs:range :Agent ;
            rdfs:label "operated by"@en .


###  https://w3id.org/rec/core/ownedBy
:ownedBy rdf:type owl:ObjectProperty ;
         owl:inverseOf :owns ;
         rdfs:range :Agent ;
         rdfs:comment "Property that defines the legal owner(s) of a given entity."@en ;
         rdfs:label "owned by"@en .


###  https://w3id.org/rec/core/owns
:owns rdf:type owl:ObjectProperty ;
      rdfs:domain :Agent ;
      rdfs:comment "Indicates that an agent is the legal owner of a given entity, e.g., a Company owns some Real Estate."@en ;
      rdfs:label "owns"@en .


###  https://w3id.org/rec/core/premisesType
:premisesType rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf :hasType ;
              rdfs:domain :Premises ;
              rdfs:range :PremisesType ;
              owl:deprecated "true"^^xsd:boolean .


###  https://w3id.org/rec/core/producedBy
:producedBy rdf:type owl:ObjectProperty ;
            owl:inverseOf :producedEvent ;
            rdfs:domain :Event ;
            rdfs:comment "The entity responsible for generating or producing the event."@en ;
            rdfs:label "produced by"@en .


###  https://w3id.org/rec/core/producedEvent
:producedEvent rdf:type owl:ObjectProperty ;
               rdfs:range :Event ;
               rdfs:comment "Event(s) generated or produced by an entity."@en ;
               rdfs:label "associated with event"@en .


###  https://w3id.org/rec/core/qkMeasurementUnit
:qkMeasurementUnit rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf :hasMeasurementUnit ;
                   rdfs:domain :QuantityKind ;
                   rdfs:range :MeasurementUnit ;
                   rdfs:comment """A unit used to specificy some Quantity Kind, such as temperature, pressure, etc.

Deprecated in favour of qudt:unit"""@en ;
                   rdfs:label "quantity kind measurement unit"@en ;
                   owl:deprecated "true"^^xsd:boolean .


###  https://w3id.org/rec/core/sensorInterface
:sensorInterface rdf:type owl:ObjectProperty ;
                 rdfs:range :SensorInterface ;
                 rdfs:label "sensor interface"@en .


###  https://w3id.org/rec/core/servedBy
:servedBy rdf:type owl:ObjectProperty ;
          owl:inverseOf :serves ;
          rdfs:comment "Indicates that a Space or Asset is served by some Sensor/Actuator or other Asset. For example: an entrance room might be served by (e.g., covered by) some camera equipment, or a conference room served by a CO2 sensor. Note that Assets can also service one another, e.g., an air-treatment Asset might serve an air diffuser Asset."@en ;
          rdfs:label "served by"@en .


###  https://w3id.org/rec/core/serves
:serves rdf:type owl:ObjectProperty ;
        rdfs:comment "The coverage or impact area of a given Asset or Sensor/Actuator. For example: an air-treatment unit might serve several Rooms or a full Building. Note that Assets can also service one another, e.g., an air-treatment Asset might serve an air diffuser Asset."@en ;
        rdfs:label "serves"@en .


###  https://w3id.org/rec/core/servesBuilding
:servesBuilding rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf :serves ;
                rdfs:range :Building ;
                rdfs:comment """Indicates how a Device serves an entire Building.

Deprecated in favour of core:servesSpace"""@en ;
                rdfs:label "serves building"@en ;
                owl:deprecated "true"^^xsd:boolean .


###  https://w3id.org/rec/core/servesBuildingComponent
:servesBuildingComponent rdf:type owl:ObjectProperty ;
                         rdfs:subPropertyOf :serves ;
                         rdfs:range :BuildingComponent ;
                         rdfs:comment """Indicates how a Device serves a particular Building Component (e.g., a Room).

Deprecated in favour of core:servesSpace"""@en ;
                         rdfs:label "serves building component"@en ;
                         owl:deprecated "true"^^xsd:boolean .


###  https://w3id.org/rec/core/servesDevice
:servesDevice rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf :serves ;
              rdfs:comment """Indicates how a Device serves another Device.

Deprecated in favour of core:servesAsset"""@en ;
              rdfs:label "serves device"@en ;
              owl:deprecated "true"^^xsd:boolean .


